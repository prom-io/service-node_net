    "@types/nedb": "^1.8.9",
    "@types/pull-stream": "^3.6.0",
    "@types/random-number": "^0.0.0",
    "@types/uuid": "^3.4.6",
    "axios": "^0.19.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "date-fns": "^2.9.0",
    "dotenv": "^8.2.0",
    "env-decorator": "^1.0.0",
    "libp2p": "^0.38.0",
    "libp2p-bootstrap": "^0.10.3",
    "libp2p-gossipsub": "^0.2.2",
    "libp2p-mdns": "^0.12.3",
